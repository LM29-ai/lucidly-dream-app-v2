{
  "name": "lucidly",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "export:embed:android": "expo export:embed --eager --platform android --dev false",
    "preinstall": "node -e \"try{console.log('[preinstall] RN gradle-plugin before:', require.resolve('@react-native/gradle-plugin/package.json'))}catch(e){console.log('[preinstall] RN gradle-plugin not installed yet')}; process.exit(0)\"",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "expo": "54.0.0",
    "expo-asset": "~11.1.0",
    "expo-constants": "~17.1.0",
    "expo-linking": "~7.1.0",
    "expo-router": "5.1.6",
    "@expo/metro-config": "^0.20.17",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.3",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.9.0",

    "@react-native/gradle-plugin": "0.73.5"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "babel-preset-expo": "^11.0.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.0",
    "patch-package": "^8.0.0"
  },
  "overrides": {
    "@react-native/gradle-plugin": "0.73.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "packageManager": "npm@10.7.0",
  "engines": {
    "node": ">=20.17.0"
  }
}